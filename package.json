    "url": "https://github.com/rogr/rog3r"
  },
  "keywords": [
    "person",
    "contact"
  ],
  "babel": {
    "presets": ["es2015"]
  },
  "author": "Rogelio Alberto",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "webpack": "^1.13.0"
  }
}
