{
  "name": "rog3r",
  "version": "1.0.8",
  "description": "Contact information and API for a cool guy named Rogelio Alberto",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./lib/rog.min.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "postinstall": "npm run build; echo 'Rog says Hello:'; cat src/me.json",
    "test": "tape test/*.js",
    "coverage": "istanbul cover tape -- test/*.js",
    "report": "npm run coverage && open coverage/lcov-report/lib/rog.js.html"
  },
  "author": "Rogelio Alberto",
  "license": "Beerware",
  "repository": {
    "type": "git",
    "url": "https://github.com/rog/rog3r"
  },
  "keywords": [
    "person",
    "contact"
  ],
  "dependencies": {
    "es6-promise": "^4.1.0",
    "fetch-everywhere": "^1.0.5"
  },
  "devDependencies": {
    "exports-loader": "^0.6.4",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.0.3",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^5.0.2",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.2.2",
    "webpack": "2.3.3",
    "yargs": "7.0.2"
  }
}
