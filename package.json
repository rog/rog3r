{
  "name": "rog3r",
  "version": "0.0.1",
  "description": "Contact Information about Rogelio Alberto",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./lib/rog.js",
  "scripts": {
    "makelib": "rm -rf lib && mkdir lib && cp src/me.json lib/me.json",
    "build": "npm run makelib && ./node_modules/.bin/babel src -d lib",
    "postinstall": "cat lib/src/me.json; say 'Rog says Hello'",
    "watch": "echo '[rog3r] Babel Starting' && onchange 'src/*.js' -- npm run build"
  },
  "author": "Rogelio Alberto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogr/rog3r"
  },
  "keywords": [
    "person",
    "contact"
  ],
  "dependencies": {
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "onchange": "^2.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
