{
  "name": "rog3r",
  "version": "1.0.1",
  "description": "Contact Information about Rogelio Alberto",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./index.js",
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel index.js -d lib",
    "prestart": "npm run build && cp me.json lib/me.json",
    "start": "node lib/index",
    "postinstall": "cat me.json; say 'Rog says Hello'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogr/rog3r"
  },
  "keywords": [
    "person",
    "contact"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Rogelio Alberto",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0"
  }
}
